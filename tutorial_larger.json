                "input1": "div.csv",
                "input2": "mult.csv",
                "output": "sum2.csv"
            },
            "InvokeNext": "finalsum"
        },
        "finalsum": {
            "FunctionName": "compute_sum_arrow",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "tutorial2",
                "input1": "sum.csv",
                "input2": "sum2.csv",
                "output": "finalsum.csv"
            },
            "InvokeNext": [

            ]
        }
